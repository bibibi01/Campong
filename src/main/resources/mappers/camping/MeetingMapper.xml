<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.campong.moim.mapper.MeetingMapper">

<resultMap type="com.multi.campong.moim.vo.MeetingMember" id="MoimMap">
	<result property="meetNo" column="MEET_NO" />
	<result property="mNo" column="USER_NO" />
</resultMap>




<insert id="insertMoim" parameterType="com.multi.campong.moim.vo.MeetingMember">

</insert>
<select id="getMeetingByNo" resultMap="MoimMap" resultType="com.multi.campong.moim.vo.MeetingMember">
	select * from MEETINGMEMBER where USER_NO=#{mNo}
</select>

</mapper>